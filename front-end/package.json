{
  "name": "front-end",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve",
    "open": "ng server --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
